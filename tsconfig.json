{
  "references": [
    {
      "path": "packages/sdk-react"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@fusionauth-sdk/*": ["packages/*/src/index.ts"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "sourceMap": true,
    "declaration": true,
    "composite": true,
    "declarationMap": true,
    "lib": ["es2022", "dom", "dom.iterable"],
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "types": ["node"]
  },
  "files": [],
  "include": []
}
