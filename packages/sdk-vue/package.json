{
  "name": "@fusionauth/vue-sdk",
  "version": "1.1.0",
  "description": "FusionAuth solves the problem of building essential security without adding risk or distracting from your primary application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "docs": "typedoc --plugin typedoc-plugin-markdown",
    "build": "vue-docgen-web-types && vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --environment jsdom --watch=false",
    "test:watch": "vitest --environment jsdom"
  },
  "repository": "https://github.com/FusionAuth/fusionauth-javascript-sdk",
  "author": "FusionAuth",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/FusionAuth/fusionauth-javascript-sdk/issues"
  },
  "homepage": "https://github.com/FusionAuth/fusionauth-javascript-sdk/tree/main/packages/sdk-vue#readme",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.2",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.4",
    "jsdom": "^24.0.0",
    "minimist": "1.2.8",
    "sass": "1.65.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "3.5.1",
    "vitest": "^1.3.1",
    "vue": "^3.0.0",
    "vue-docgen-web-types": "0.1.8",
    "vue-tsc": "^1.8.5",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "@fusionauth-sdk/core": "*"
  },
  "files": [
    "dist/*"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/vue-fusionauth.js",
  "module": "./dist/vue-fusionauth.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-fusionauth.js"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "web-types": "./web-types.json"
}
